{
  "class": "AS3",
  "action": "deploy",
  "persist": true,
  "declaration": {
    "class": "ADC",
    "schemaVersion": "3.12.0",
    "id": "example-declaration-01",
    "label": "Gitlab",
    "remark": "HTTP Application Service",
    "target": {
      "address": "10.1.1.8"
    },
    "gitlab": {
      "class": "Tenant",
      "MyWebApp10http": {
        "class": "Application",
        "template": "http",
        "statsProfile": {
          "class": "Analytics_Profile",
          "collectClientSideStatistics": true,
          "collectOsAndBrowser": false,
          "collectMethod": false
        },
        "serviceMain": {
          "class": "Service_HTTP",
          "virtualAddresses": [
            "10.1.10.111"
          ],
          "pool": "web_pool",
          "profileAnalytics": {
            "use": "statsProfile"
          }
        },
        "web_pool": {
          "class": "Pool",
          "monitors": [
            "http"
          ],
          "members": [
            {
              "servicePort": 80,
              "serverAddresses": [
                "10.1.20.110"
              ],
              "shareNodes": true
            }
          ]
        }
      }
    }
  }
}